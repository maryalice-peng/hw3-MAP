<h1><%= @place["name"]%></h1>

<h2>Entries</h2>

<ul>
<% for entry in @entries %> 
  <h3> <%= entry["title"] %> </h3>
  <li><%= entry["description"] %></li>
  <li><%= entry["occurred_on"] %></li>
  <% end %>
</ul>

<p>
  <a href="/entries/new?place_id=<%= @place["id"]%>">New Entry</a>
</p>

<p>
  <a href="/places">Back to All Places</a>
</p>

